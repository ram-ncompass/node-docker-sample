{
    "family": "ram-task-fg", 
    "networkMode": "awsvpc", 
    "containerDefinitions": [
        {
            "name": "ram-container", 
            "image": "368355641188.dkr.ecr.ap-southeast-2.amazonaws.com/nodejs-ram:tag_value", 
            "executionRoleArn": "arn:aws:iam::368355641188:role/ecsTaskExecutionRole",
            "portMappings": [
                {
                    "containerPort": 3001, 
                    "hostPort": 3001, 
                    "protocol": "tcp"
                }
            ], 
            "essential": true
        }
    ], 
    "requiresCompatibilities": [
        "FARGATE"
    ], 
    "cpu": "256", 
    "memory": "512"
}